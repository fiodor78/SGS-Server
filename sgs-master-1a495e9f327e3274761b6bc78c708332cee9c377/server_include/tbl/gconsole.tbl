GCONSOLE(CONSOLE_STATS,ConsoleProcessStats,"Display server statistics\r\nsyntax: stats (warning|error|info|firewall|used|all)")
GCONSOLE(CONSOLE_MEMORY,ConsoleProcessMemory,"Display information about memory allocations\r\nsyntax: memory")
GCONSOLE(CONSOLE_FIREWALL,ConsoleProcessFireWall,"Manage buildin server firewall\r\nsyntax: firewall (ADD|DELETE|EXIST|SIZE|SHOW|LIMITS|RESET|LOAD_SQL|SET LIMIT)\r\nwhere:\r\nADD:       add x(0-255).x(0-255).x(0-255).x(0-255)\r\n           add new element to firewall\r\nDELETE:    delete x(0-255).x(0-255).x(0-255).x(0-255)\r\n           delete existing element from firewall\r\nEXIST:     delete x(0-255).x(0-255).x(0-255).x(0-255)\r\n           check if element exist in firewall\r\nSIZE:      size\r\n           return elements in firewall\r\nLIMITS:     limits\r\n           show dynamic limits\r\nSET LIMIT: set limit ((1|minute)|(5|5 minutes)|(15|quarter)|(60|hour)) to x(0-1000)\r\n           set dynamic limits to value x\r\nRESET:       reset [dynamic|static]\r\n           reset firewall or static or dynamic part of firewall\r\nLOAD_SQL:  load_sql\r\n           reload data from sql\r\n")
GCONSOLE(CONSOLE_SERVICES,ConsoleServices,"Manage services\r\nsyntax: services (SHOW|REINIT|TICKETQUEUELIMIT)\r\nwhere:\r\nSHOW:      show\r\n           show services status\r\nREINIT:    reinit [GROUP|NAME|ADDRESS|PORT]\r\n           reinit service\r\nTICKETQUEUELIMIT:    ticketqueuelimit [new_value]\r\n           show/set new ticket queue size limit\r\nwhere:\r\nGROUP:     group x(0-100)\r\n           reinit services from group x\r\nNAME:      name x\r\n           reinit services with given name\r\nADDRESS:   address x(0-255).x(0-255).x(0-255).x(0-255)\r\n           reinit services connected to given address\r\nPORT:      port x(0-10000)\r\n           reinit services connected to given port\r\n")
GCONSOLE(CONSOLE_ACTION,ConsoleAction,"Call small actions from all threads\r\nsyntax: action x")
GCONSOLE(CONSOLE_SQLSTATS,ConsoleProcessSQLStats,"Display SQL query statistics\r\nsyntax: sqlstats [CSV|SLOWEST]")
GCONSOLE(CONSOLE_COREDUMP,ConsoleProcessCoreDump,"Dump core file from current process\r\nsyntax: coredump")
GCONSOLE(CONSOLE_LOGIC,ConsoleProcessLogic,"Manage game logic\r\nsyntax: logic (RELOAD|STATS|CLEARSTATS)\r\nRELOAD:      reload\r\n           reload Lua VM and all scripts\r\nSTATS:      stats\r\n           show performance statistics of game logic\r\nCLEARSTATS:      clearstats\r\n           zero performance statistics of game logic")
